{
    "editor.tabSize": 2,
    "workbench.tree.indent": 16,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,

        //Node
        "**/dist": false,
        "**/node_modules": true,
        "**/bulid": true,
        "**/package-lock.json": true,
        "**/tsconfig.app.json": false,
        "**/tsconfig.node.json": false,
        "**/uno.config.ts": false,
        "**/vite.config.ts": false,
        
        //Git
        "**/.gitattributes": true,

        //Gradle
        "**/build": true,
        "**/.gradle": true,
        "**/gradle": true,
        "**/gradle/wrapper": true,
        "**/gradlew": true,
        "**/gradlew.bat": true,

        //Eclipse
        "**/bin": true,
        "**/.settings": true,
        "**/.project": true,
        "**/.classpath": true,
        "**/.factorypath": true,
    },
    "typescript.preferences.importModuleSpecifier": "non-relative",
    "typescript.preferences.importModuleSpecifierEnding": "js"
}